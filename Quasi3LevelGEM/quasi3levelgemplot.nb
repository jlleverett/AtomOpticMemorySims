Cell[TextData[{
 "Plots input and output optical intensities ",
 Cell[BoxData[
  RowBox[{"|", 
   RowBox[{"\[ScriptCapitalE]", 
    RowBox[{"(", "t", ")"}]}], 
   SuperscriptBox["|", "2"]}]],
  CellChangeTimes->{{3.731801721806876*^9, 3.731801917129793*^9}, {
